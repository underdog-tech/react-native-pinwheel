{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./lib",
    "strict": true,
    "lib": ["dom", "es5", "es2017"],
    "jsx": "react-native",
    "skipLibCheck": true,
    "moduleResolution": "node",
    "allowJs": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src"
  ],
  "exclude": ["node_modules", "lib"]
}
